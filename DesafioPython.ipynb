    "import pandas as pd;\n",
    "import requests;\n",
    "\n",
    "url = 'http://localhost:8080/demo'\n",
    "r = requests.get(url)\n",
    "json = r.json()\n",
    "df = pd.DataFrame(json)\n",
    "\n",
    "IMC = (df['peso'] / df['altura'] ** 2).round(2) \n",
    "display(IMC)"
